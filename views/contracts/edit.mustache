<div class="container container-main-content scene">

  <div class="row">
    <header class="scene-header page-header">
      <h1 class="scene-header-headline">{{ contract.branch }} <small>Vertrag bearbeiten {{ contract.id }}</small></h1>
      <small>
        letzte Änderung: <time datetime="2014-07-15 09:36">vor 3 Tagen</time>
        (<a href="#" class="h-card">01 Gosen</a>)
      </small>
    </header>
    <form class="form-horizontal" role="form" action="index.html">
      <div class="scene-agents">
        <div class="form-group">
          <label for="agent-original" class="control-label">Bestand</label>
          <div class="fields">
            <select id="agent-original" class="form-control">
              <option>00 Thomas Gosen</option>
              <option>01 &hellip;</option>
            </select>
          </div>
        </div>
        <div class="form-group">
          <label for="agent-current" class="control-label">Abschluss</label>
          <div class="fields">
            <select id="agent-current" class="form-control">
              <option>00 Thomas Gosen</option>
              <option>01 &hellip;</option>
            </select>
          </div>
        </div>
      </div>
    </form>
  </div>

  <div class="row">
    <div class="col-md-12">

      <div class="contract-details">

        <ul class="nav nav-tabs scene-contents" role="tablist">
          <li><a href="#kfz-fahrzeugdaten" class="kbd" role="tab" data-toggle="tab">Fahrzeugdaten</a></li>
          <li><a href="#kfz-nutzung" class="kbd" role="tab" data-toggle="tab">Nutzung</a></li>
          <li><a href="#kfz-versSchutz" class="kbd" role="tab" data-toggle="tab">Versicherungsschutz</a></li>
        </ul>

        <div>
          <div id="kfz-fahrzeugdaten" class="tab-pane">

            <form class="form-horizontal scene-form" role="form">
              <fieldset class="scene-fieldset">
                <div class="form-group">
                  <label for="kfz-fahrzeugart" class="control-label">Fahrzeugart</label>
                  <div class="fields">
                    <select id="kfz-fahrzeugart" class="form-control">
                      <option>{{ contract.fahrzeugdaten.fahrzeugart }}</option>
                      <option>…</option>
                    </select>
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-kennzeichen" class="control-label">Kennzeichen</label>
                  <div class="fields form-inline">
                    <input type="text" id="kfz-kennzeichen" class="form-control" value="{{ contract.fahrzeugdaten.kennzeichen }}">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-plz" class="control-label"><abbr title="Postleitzahl">PLZ</abbr> / Ort</label>
                  <div class="fields">
                    <input type="text" id="kfz-plz" class="form-control">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-herstellernr" class="control-label">Hersteller-Nr.</label>
                  <div class="fields form-inline">
                    <div class="input-group">
                      <input type="search" id="kfz-herstellernr" class="form-control" value="{{ contract.fahrzeugdaten.hsn }}">
                    </div>
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-typschluesselnr" class="control-label">Typschlüssel-Nr. <a class="control-label-help-link" data-toggle="modal" data-target=".modal-help-example" href="#"><i class="icon-question"></i></a></label>
                  <div class="fields">
                    <input type="search" id="kfz-typschluesselnr" class="form-control" value="{{ contract.fahrzeugdaten.typschl }}">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-erstzulassung" class="control-label">Erstzulassung</label>
                  <div class="fields">
                    <input type="month" id="kfz-erstzulassung" class="form-control" value="{{ contract.fahrzeugdaten.erstzulassung }}">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-erwerb" class="control-label">Erwerb <abbr>VN</abbr> / <abbr>FH</abbr></label>
                  <div class="fields">
                    <input type="month" id="kfz-erwerb" class="form-control">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-fahrgestell" class="control-label">Fahrgestell-Nr.</label>
                  <div class="fields">
                    <input type="text" id="kfz-fahrgestell" class="form-control" value="{{ contract.fahrzeugdaten.fahrgestell }}">
                  </div>
                </div>
              </fieldset>

              <fieldset class="scene-fieldset">
                <div class="form-group form-group-multiple">
                  <label for="kfz-saison-von" class="control-label">Saison von</label>
                  <div class="col-sm-3 form-group-multiple-item">
                    <input type="month" id="kfz-saison-von" class="form-control">
                  </div>
                  <label for="kfz-saison-bis" class="control-label">Saison bis</label>
                  <div class="col-sm-3 form-group-multiple-item">
                    <input type="month" id="kfz-saison-bis" class="form-control">
                  </div>
                </div>

                <div class="form-group form-group-multiple">
                  <label for="kfz-kennzeichen-art" class="control-label">KFZ-Kennzeichen-Art</label>
                  <div class="col-sm-3 form-group-multiple-item">
                    <select id="kfz-fahrzeugart" class="form-control">
                      <option>{{ contract.fahrzeugdaten.kennzeichenart }}</option>
                      <option>…</option>
                    </select>
                  </div>

                  <label for="kfz-wechselkennzeichen" class="control-label">Wechselkennzeichen</label>
                  <div class="col-sm-3 form-group-multiple-item">
                    <label class="radio-inline">
                      <input name="kfz-wechselkennzeichen" value="nein" checked="" type="radio">
                      nein
                    </label>
                    <label class="radio-inline">
                      <input name="kfz-wechselkennzeichen" value="ja" type="radio">
                      ja
                    </label>
                  </div>
                </div>

                <div class="form-group form-group-multiple">
                  <label for="kfz-leistung-kw" class="control-label">Leistung (kW)</label>
                  <div class="col-sm-3 form-group-multiple-item">
                    <input type="text" id="kfz-leistung-kw" class="form-control">
                  </div>
                  <label for="kfz-leistung-ps" class="control-label">Leistung (PS)</label>
                  <div class="col-sm-3 form-group-multiple-item">
                    <input type="text" id="kfz-leistung-ps" class="form-control" value="{{ contract.fahrzeugdaten.fahrzeugstaerkePS }}">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-austauschmotor" class="control-label">Austauschmotor</label>
                  <div class="col-sm-3 form-group-multiple-item">
                    <label class="radio-inline">
                      <input name="kfz-austauschmotor" value="nein" checked="" type="radio">
                      nein
                    </label>
                    <label class="radio-inline">
                      <input name="kfz-austauschmotor" value="ja" type="radio">
                      ja
                    </label>
                  </div>
                </div>

              </fieldset>
            </form>
          </div>

          <div id="kfz-nutzung" class="tab-pane">
            <form class="form-horizontal scene-form" role="form">
              <fieldset class="scene-fieldset">
                <div class="form-group">
                  <label for="kfz-beliebigeFahrer" class="control-label">beliebige Fahrer</label>
                  <div class="fields">
                    <input type="text" id="kfz-beliebigeFahrer" class="form-control" value="{{ contract.nutzung.beliebigeFahrer }}">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-abweichenderFahrzeughalter" class="control-label">abweichender Fahrzeughalter</label>
                  <div class="col-sm-3 form-group-multiple-item">
                    <label class="radio-inline">
                      <input name="kfz-abweichenderFahrzeughalter" value="nein" checked="" type="radio">
                      nein
                    </label>
                    <label class="radio-inline">
                      <input name="kfz-abweichenderFahrzeughalter" value="ja" type="radio">
                      ja
                    </label>
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-fahrleistungKm" class="control-label">Fahrleistung (jährl./Saison)</label>
                  <div class="fields">
                    <input type="text" id="kfz-fahrleistungKm" class="form-control" value="{{ contract.nutzung.fahrleistungKm }}">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-kilometerstand" class="control-label">Kilometerstand</label>
                  <div class="fields">
                    <input type="text" id="kfz-kilometerstand" class="form-control" value="{{ contract.nutzung.kilometerstand }}">
                  </div>
                </div>
              </fieldset>

              <fieldset class="scene-fieldset">
                <div class="form-group">
                  <label for="kfz-nutzung" class="control-label">Fahrzeugnutzung</label>
                  <div class="fields">
                    <input type="text" id="kfz-nutzung" class="form-control" value="{{ contract.nutzung.nutzung }}">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-selbstGenEigentum" class="control-label">selbstgen. Wohneigentum</label>
                  <div class="col-sm-3 form-group-multiple-item">
                    <label class="radio-inline">
                      <input name="kfz-selbstGenEigentum" value="nein" checked="" type="radio">
                      nein
                    </label>
                    <label class="radio-inline">
                      <input name="kfz-selbstGenEigentum" value="ja" type="radio">
                      ja
                    </label>
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-wohneigentumart" class="control-label">Wohneigentumsart</label>
                  <div class="fields">
                    <input type="text" id="kfz-wohneigentumart" class="form-control" value="{{ contract.nutzung.wohneigentumart }}">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-nachtAbstellplatz" class="control-label">nächtlicher Abstellplatz</label>
                  <div class="fields">
                    <input type="text" id="kfz-nachtAbstellplatz" class="form-control" value="{{ contract.nutzung.nachtAbstellplatz }}">
                  </div>
                </div>
              </fieldset>
            </form>
          </div>

          <div id="kfz-versSchutz" class="tab-pane">
            <form class="form-horizontal scene-form" role="form">
              <fieldset class="scene-fieldset">
                <div class="form-group">
                  <label for="kfz-rahmenvertrag" class="control-label">Rahmenvertrag</label>
                  <div class="fields">
                    <input type="text" id="kfz-rahmenvertrag" class="form-control" value="{{ contract.versSchutz.rahmenvertrag }}">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-tarifgruppe" class="control-label">Tarifgruppe</label>
                  <div class="fields">
                    <input type="text" id="kfz-tarifgruppe" class="form-control" value="{{ contract.versSchutz.tarifgruppe }}">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-versBeginn" class="control-label">Versicherungsbeginn</label>
                  <div class="fields">
                    <input type="text" id="kfz-versBeginn" class="form-control" value="{{ contract.versSchutz.versBeginn }}">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-zahlungsweise" class="control-label">Zahlungsweise</label>
                  <div class="fields">
                    <input type="text" id="kfz-zahlungsweise" class="form-control" value="{{ contract.versSchutz.zahlungsweise }}">
                  </div>
                </div>
              </fieldset>

              <fieldset class="scene-fieldset">
                <div class="form-group">
                  <label for="kfz-harfpflichtSFR" class="control-label">Haftpflicht Schadensfreiheitrabatt</label>
                  <div class="fields">
                    <input type="text" id="kfz-harfpflichtSFR" class="form-control" value="{{ contract.versSchutz.haftpflichSFR }}">
                  </div>
                </div>

                <div class="form-group">
                  <label for="kfz-volkaskoSFR" class="control-label">Vollkasko-Schadensfreiheitrabatt</label>
                  <div class="fields">
                    <input type="text" id="kfz-volkaskoSFR" class="form-control" value="{{ contract.versSchutz.volkaskoSFR }}">
                  </div>
              </fieldset>
            </form>

          </div>
        </div>
      </div>

    </div>
  </div>

  <hr>

  <div class="row">
    <div class="col-md-12">
      {{> scene_summary }}
    </div>
  </div>

</div>
